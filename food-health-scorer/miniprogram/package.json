{
  "name": "food-health-scorer-miniprogram",
  "version": "1.0.0",
  "description": "食品健康评分小程序",
  "main": "main.js",
  "scripts": {
    "dev:mp-weixin": "uni -p mp-weixin",
    "build:mp-weixin": "uni build -p mp-weixin",
    "dev:h5": "uni -p h5",
    "build:h5": "uni build -p h5",
    "dev:h5-vite": "vite --config vite.config.js",
    "dev:h5-open": "uni -p h5 --browser chrome",
    "dev:h5-debug": "cross-env UNI_PLATFORM=h5 NODE_ENV=development vite --config vite.config.js --mode development --host"
  },
  "keywords": [
    "uni-app",
    "vue3",
    "food-health",
    "miniprogram"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dcloudio/uni-app": "^3.0.0",
    "@dcloudio/uni-app-plus": "^3.0.0",
    "@dcloudio/uni-components": "^3.0.0",
    "@dcloudio/uni-h5": "^3.0.0",
    "@dcloudio/uni-mp-alipay": "^3.0.0",
    "@dcloudio/uni-mp-baidu": "^3.0.0",
    "@dcloudio/uni-mp-kuaishou": "^3.0.0",
    "@dcloudio/uni-mp-lark": "^3.0.0",
    "@dcloudio/uni-mp-qq": "^3.0.0",
    "@dcloudio/uni-mp-toutiao": "^3.0.0",
    "@dcloudio/uni-mp-weixin": "^3.0.0",
    "@dcloudio/uni-quickapp-webview": "^3.0.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.0.0",
    "@dcloudio/uni-automator": "^3.0.0",
    "@dcloudio/uni-cli-shared": "^3.0.0",
    "@dcloudio/uni-stacktracey": "^3.0.0",
    "@dcloudio/vite-plugin-uni": "^3.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "sass": "^1.56.1",
    "vite": "^4.0.0"
  }
